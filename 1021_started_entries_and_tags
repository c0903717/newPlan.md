        PanelVerbal2.st2.SetFont(font)

        self.SetBackgroundColour((0xbb,0xdd,0xbb))

        Sizer = wx.BoxSizer(wx.VERTICAL)
        Sizer.Add(PanelVerbal2.st, 0, wx.ALIGN_CENTER|wx.ALL, 15)
        Sizer.Add(PanelVerbal2.st2, 0, wx.ALIGN_CENTER|wx.ALL, 15)
        
        self.SetSizerAndFit(Sizer)
    def ChangeTitle(self,txt,event=None):
