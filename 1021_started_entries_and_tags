        dlg.Destroy()
    def MediumButtonSetupVisual(self,event=None):
        #make reset button functionality
        self.panel_three.buttonArr = [self.panel_three.newMedium1, self.panel_three.newMedium2, self.panel_three.newMedium3, self.panel_three.newMedium4, self.panel_three.newMedium5,self.panel_three.newMedium6, self.panel_three.newMedium7, self.panel_three.newMedium8,self.panel_three.newMedium9,self.panel_three.newMedium10]        
        self.panel_three.buttonArr[self.panel_three.counter].SetLabelText(self.panel_three.txt)
        MyForm.MediumButtonBindingVisual(self,self.panel_three.buttonArr[self.panel_three.counter])
        if PanelThree.counter<9 and PanelThree.counter>-1:
            PanelThree.counter+=1
        if PanelThree.counter==9:
            PanelThree.counter=--1
    def MediumButtonSetupVerbal(self,event=None):
        #make reset button functionality
        self.panel_two.buttonArr = [self.panel_two.newMedium1, self.panel_two.newMedium2, self.panel_two.newMedium3, self.panel_two.newMedium4, self.panel_two.newMedium5,self.panel_two.newMedium6, self.panel_two.newMedium7, self.panel_two.newMedium8,self.panel_two.newMedium9,self.panel_two.newMedium10]        
        self.panel_two.buttonArr[self.panel_two.counter].SetLabelText(self.panel_two.txt)
        MyForm.MediumButtonBindingVerbal(self,self.panel_two.buttonArr[self.panel_two.counter])
        if PanelTwo.counter<9 and PanelTwo.counter>-1:
            PanelTwo.counter+=1
        if PanelTwo.counter==9:
            PanelTwo.counter=-1
    def MediumButtonBindingVerbal(self,btn,event=None):
        num=self.panel_three.counter+1
        if num==1:
            btn.Bind(wx.EVT_BUTTON, self.MediumVerbal1)
            PanelVerbal1.ChangeTitle(self,self.panel_two.txt)
        elif num==2:
            btn.Bind(wx.EVT_BUTTON, self.MediumVerbal2)
            PanelVerbal2.ChangeTitle(self,self.panel_two.txt)
        elif num==3:
            btn.Bind(wx.EVT_BUTTON, self.MediumVerbal3)
            PanelVerbal3.ChangeTitle(self,self.panel_two.txt)
        elif num==4:
            btn.Bind(wx.EVT_BUTTON, self.MediumVerbal4)
            PanelVerbal4.ChangeTitle(self,self.panel_two.txt)
        elif num==5:
            btn.Bind(wx.EVT_BUTTON, self.MediumVerbal5)
            PanelVerbal6.ChangeTitle(self,self.panel_two.txt)
        elif num==6:
            btn.Bind(wx.EVT_BUTTON, self.MediumVerbal6)
            PanelVerbal6.ChangeTitle(self,self.panel_two.txt)
        elif num==7:
            btn.Bind(wx.EVT_BUTTON, self.MediumVerbal7)
            PanelVerbal7.ChangeTitle(self,self.panel_two.txt)
        elif num==8:
            btn.Bind(wx.EVT_BUTTON, self.MediumVerbal8)
            PanelVerbal8.ChangeTitle(self,self.panel_two.txt)
        elif num==9:
            btn.Bind(wx.EVT_BUTTON, self.MediumVerbal9)
            PanelVerbal9.ChangeTitle(self,self.panel_two.txt)
        elif num==10:
            btn.Bind(wx.EVT_BUTTON, self.MediumVerbal10)
            PanelVerbal10.ChangeTitle(self,self.panel_two.txt)
    def MediumButtonBindingVisual(self,btn,event=None):
