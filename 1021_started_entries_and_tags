    def GetMediumResponseVisual(self, event=None):
        dlg = GetData1(parent = self)  
        dlg.ShowModal
        print(dlg.ShowModal())
        self.panel_three.txt=(dlg.result_name)
        self.panel_three.txt2=(dlg.result_surname)
        print(self.panel_three.txt)
        print(self.panel_three.txt2)
        if (dlg.result_name): 
            self.MediumButtonSetupVisual()
        dlg.Destroy()
    def GetMediumResponseVerbal(self, event=None):
        dlg = GetData1(parent = self)  
        dlg.ShowModal
        print(dlg.ShowModal())
        self.panel_two.txt=(dlg.result_name)
        self.panel_two.txt2=(dlg.result_surname)
