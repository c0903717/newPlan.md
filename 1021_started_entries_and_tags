        PanelVerbal8.st=wx.StaticText(self, label="PanelVerbal8")
        font = PanelVerbal8.st.GetFont()
        font.PointSize += 5
        PanelVerbal8.st.SetFont(font)
        PanelVerbal8.st2=wx.StaticText(self, label="Medium Description")
        font = PanelVerbal8.st2.GetFont()
        font.PointSize += 5
        PanelVerbal8.st2.SetFont(font)

        self.SetBackgroundColour((0xbb,0xdd,0xbb))

        Sizer = wx.BoxSizer(wx.VERTICAL)
        Sizer.Add(PanelVerbal8.st, 0, wx.ALIGN_CENTER|wx.ALL, 15)
        Sizer.Add(PanelVerbal8.st2, 0, wx.ALIGN_CENTER|wx.ALL, 15)
        
        self.SetSizerAndFit(Sizer)
    def ChangeTitle(self,txt,event=None):
        PanelVerbal8.st.SetLabelText(txt)
